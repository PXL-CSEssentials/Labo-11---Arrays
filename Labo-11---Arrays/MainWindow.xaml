<Window x:Class="Labo_11___Arrays.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Labo_11___Arrays"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button x:Name="outputButton" Grid.Row="0" Content="Output" HorizontalAlignment="Center" Width="500" Margin="0 10 0 10" Click="outputButton_Click"/>
        <TextBox x:Name="outputTextBox" Grid.Row="1" HorizontalAlignment="Center" TextWrapping="Wrap" Text="" Width="500"/>
    </Grid>
</Window>
